<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity CarOnRoute.
    -->
    <changeSet id="20170621192109-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="car_id"
                                 baseTableName="car_on_route"
                                 constraintName="fk_car_on_route_car_id"
                                 referencedColumnNames="id"
                                 referencedTableName="car"/>

        <addForeignKeyConstraint baseColumnNames="route_id"
                                 baseTableName="car_on_route"
                                 constraintName="fk_car_on_route_route_id"
                                 referencedColumnNames="id"
                                 referencedTableName="bus_route"/>

    </changeSet>
</databaseChangeLog>
